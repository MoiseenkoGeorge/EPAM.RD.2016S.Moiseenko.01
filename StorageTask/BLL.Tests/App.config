<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <configSections>
  <section name="UserServices" type="BLL.Configuration.UserServiceConfigSection, BLL"/>
  </configSections>

  <UserServices>
    <Services>
      <Service 
        name="1"
        isMaster="true"
        appDomainName ="masterDomain"
        transmitter = "BLL.Services.Network.UdpSender"
               />
      <Service 
        name="2" 
        appDomainName ="slaveDomain" 
        transmitter = "BLL.Services.Network.UdpReceiver"
        />
      <Service
        name="3"
        appDomainName="slaveDomain"
        transmitter = "BLL.Services.Network.UdpReceiver"
        />
      
    </Services>
  </UserServices>
  
  
  <appSettings>
    <add key="FileName" value="Users.xml"/>
    <add key="TraceSourceName" value="CustomTraceSource"/>
    <add key = "Port" value="11000"/>
  </appSettings>

    <system.diagnostics>
      <trace autoflush="true" />
      <sources>
        <source name="CustomTraceSource"
            switchName="CustomSwitch"
            switchType="System.Diagnostics.SourceSwitch" >
          <listeners>
            <add name="textListener"
              type="System.Diagnostics.TextWriterTraceListener"
              initializeData ="D:\George_Moiseenko\logFile.txt"
              traceOutputOptions="DateTime"/>
            <remove name ="Default" />
          </listeners>
        </source>
      </sources>
      <switches>
        <add name="CustomSwitch" value="Information" />
        <add name="Activate" value="1"/>
      </switches>
    </system.diagnostics>
  
</configuration>